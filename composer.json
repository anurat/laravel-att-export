{
    "name": "anurat/laravel-att-export",
    "description": "A package for exporting attendance database",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anurat Chapanond",
            "email": "anurat@gmail.com"
        }
    ],
    "require": {
        "php": "^7|^8",
        "laravel/framework": "^7"
    },
    "require-dev": {
        "orchestra/testbench": "6.0",
        "phpunit/phpunit": "8.5"
    },

    "autoload": {
        "psr-4": {
            "Anurat\\AttExport\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anurat\\AttExport\\Tests\\": "tests"
        }
    },
    
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Anurat\\AttExport\\AttExportServiceProvider"
            ]
        }
    }
}
